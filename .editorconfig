# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4

[*.js*,*.ts*,*.hs]
indent_size = 2

[makefile]
indent_style = tabs

[coc-settings.json]
trim_trailing_whitespace = false
