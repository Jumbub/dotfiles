#!/usr/bin/env bash

alias vim="nvim"
alias v="nvim"

alias vinit="nvim $HOME/.config/nvim/init.vim"
alias valiases="nvim $HOME/.aliases"
alias vinstall="nvim $HOME/scripts/install"
alias vrc="nvim $HOME/.zshrc"
alias vkitty="nvim $HOME/.config/kitty/kitty.conf"

alias vmeg="cd $HOME/repos/megatron && vim && cd -"
alias vcat="cd $HOME/repos/catalyst/packages/catalyst && vim && cd -"

alias rsource="source $HOME/.zshrc"
alias rinstall="$HOME/scripts/install"

source $HOME/.dotFunctions
